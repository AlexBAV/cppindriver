		<IncludePath>C:\Program Files (x86)\Windows Kits\10\Include\10.0.26100.0\km;$(SolutionDir)</IncludePath>
	</PropertyGroup>
	<ItemDefinitionGroup>
		<ClCompile>
			<WarningLevel>Level4</WarningLevel>
