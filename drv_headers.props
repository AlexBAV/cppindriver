		<Link>
			<AdditionalDependencies>ntoskrnl.lib;hal.lib</AdditionalDependencies>
			<IgnoreAllDefaultLibraries>true</IgnoreAllDefaultLibraries>
			<AdditionalOptions>/driver:WDM %(AdditionalOptions)</AdditionalOptions>
		</Link>
	</ItemDefinitionGroup>
	<ItemGroup />
</Project>